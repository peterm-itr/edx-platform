<div>
    <h2><%- model.get('title') %></h2>
    <p><%- model.get('description') %></p>
    <form id="upload-file-form" method="post" action="<%= model.get('url') %>" enctype="multipart/form-data">
        <a href="#" class="choose-file-button"><%- gettext('Choose File') %></a>
        <input class="file_field" name="uploaded-file" type="file" accept="<%- model.get('extension') %>"/>
    </form>
    <div class="result"></div>
</div>